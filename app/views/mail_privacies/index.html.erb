<h1>Listing mail_privacies</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Default</th>
    <th>User type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mail_privacies.each do |mail_privacy| %>
  <tr>
    <td><%= mail_privacy.description %></td>
    <td><%= mail_privacy.default %></td>
    <td><%= mail_privacy.user_type %></td>
    <td><%= link_to 'Show', mail_privacy %></td>
    <td><%= link_to 'Edit', edit_mail_privacy_path(mail_privacy) %></td>
    <td><%= link_to 'Destroy', mail_privacy, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

